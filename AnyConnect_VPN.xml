			<HostAddress>vpn2.nidec-motor.com</HostAddress>
		</HostEntry>
		<HostEntry>
			<HostName>EMEA-AMSTERDAM-GATEWAY</HostName>
			<HostAddress>vpn3.nidec-motor.com</HostAddress>
		</HostEntry>
		<HostEntry>
			<HostName>APAC-SINGAPORE-GATEWAY</HostName>
			<HostAddress>ravpn.nidec-motor.com</HostAddress>
		</HostEntry>
	</ServerList>
</AnyConnectProfile>
